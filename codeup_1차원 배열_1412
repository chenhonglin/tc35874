#include <stdio.h>
#include <stdlib.h>
#pragma warning(disable : 4996)

#define SIZE 92

int main() {
	char str[SIZE] = {};
	int i,x,y,j;
	int number[123] = {};
	fgets(str, SIZE, stdin);
	for (i = 0; i < SIZE; i++) {
		for (j = 'a'; j <= 'z'; j++) {
			if (str[i] == j)
				number[j]++;
		 }
	}
	for (i = 97; i <= 122; i++) {
		printf("%c:%d\n", i, number[i]);
	}
		return 0;
}
