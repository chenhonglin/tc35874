#include <stdio.h>

#pragma warning(disable : 4996)


int main()
{
	int num[51];
	int i, j, k, sum1, sum2,m;
	scanf("%d", &k);
	sum1 = 0;
	sum2 = 0;
	for (i = 1; i < k; i++) {
		scanf("%d", &num[i]);
		sum1 = sum1 + num[i];
	}
	for (j = 1; j <= k; j++) {
		sum2 = sum2 + j;
	}
	m = sum2 - sum1;
	printf("%d", m);

	return 0;
}
